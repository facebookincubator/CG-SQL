  INSERT INTO sql_temp(id, sql) VALUES(1, stmt);
  DECLARE C CURSOR FOR EXPLAIN QUERY PLAN
  SELECT *
    FROM t1
    WHERE name = 'Nelly' AND id IN (SELECT id
    FROM t2
